@page "/popup.html"
@inherits BasePage

<MudGrid>
    <MudItem xs="12">
        <MudPaper Width="320px" Elevation="0" Square Class="d-flex align-center justify-center mud-width-full py-8">
            <MudCard>
                <MudCardContent>
                    <MudText Typo="Typo.h6">欢迎使用LeaFLib浏览器插件</MudText>
                    <MudText Typo="Typo.body2">The quick, brown fox jumps over a lazy dog.</MudText>
                </MudCardContent>
                <MudCardActions>
                    <MudLink Typo="Typo.button" Href="https://github.com/LeaFrock/LeaFLib.BrowserExt" Underline="Underline.Always">访问项目源码地址</MudLink>
                </MudCardActions>
            </MudCard>
        </MudPaper>
    </MudItem>
</MudGrid>